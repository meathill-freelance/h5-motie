@keyframes float
  0%
    box-shadow: 0 .25rem 1rem 0 rgba(0,0,0,0.6);
    transform: translate3d(0,0,0);
  50%
    box-shadow: 0 1.5rem 1rem 0 rgba(0,0,0,0.2);
    transform: translate3d(0,-1.25rem,0);
  100%
    box-shadow: 0 .25rem 1rem 0 rgba(0,0,0,0.6);
    transform: translate3d(0,0,0);

.float
  animation: float 6s ease-in-out infinite